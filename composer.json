{
    "name": "adnduweb/ci4_page",
    "type": "library",
    "description": "An Example of CodeIgniter 4 Blog module",
    "keywords": [
        "ci4",
        "codeigniter4",
        "codeigniter"
    ],
    "homepage": "https://github.com/adnduweb/ci4_page",
    "license": "MIT",
    "authors": [{
        "name": "Fabrice Loru",
        "email": "contacta@adnduweb.com",
        "homepage": "https://www.adnduweb.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.2",
        "adnduweb/ci4_page": "^1"
        "adnduweb/ci4_menu": "^1"
    },
    "require-dev": {
        "codeigniter4/codeigniter4-standard": "^1.0.1",
        "codeigniter4/framework": "^4",
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adnduweb\\Ci4_blog\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "analyze": "phpstan analyze src --level=max",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    }
}
